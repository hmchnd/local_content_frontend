<mvc:View
    controllerName="com.kpo.supplierreport.controller.View1"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:t="sap.ui.table"
    xmlns:rowmodes="sap.ui.table.rowmodes"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
    <NavContainer id="wizardNavContainer">
        <pages>
            <Page
                id="wizardContentPage"
                showHeader="false"
            >
                <content>
                    <form:SimpleForm
                        id="SimpleFormChange354"
                        editable="true"
                        layout="ResponsiveGridLayout"
                        labelSpanXL="2"
                        labelSpanL="2"
                        labelSpanM="2"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="5"
                        emptySpanL="5"
                        emptySpanM="5"
                        emptySpanS="0"
                        columnsXL="1"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="false"
                    >
                        <Label
                            id="_IDGenLabel"
                            text="Company Name"
                            required="true"
                        />
                        <Input
                            id="inpc1"
                            value="Kpo"
                            editable="false"
                        />
                        <Label
                            id="_IDGenLab"
                            text="Reporting Peroid"
                            required="true"
                        />
                        <Input
                            id="inpc2"
                            value="1st quarter 2024"
                            editable="false"
                        />
                    </form:SimpleForm>
                    <Wizard
                        id="CreateProductWizard"
                        class="sapUiResponsivePadding--header sapUiResponsivePadding--content"
                        complete="wizardCompletedHandler"
                    >
                        <WizardStep
                            id="ProductTypeStep"
                            title="Report on purchased goods, works and services"
                            validated="true"
                        >
                            <t:Table
                                id="itemsTable1"
                                fixedColumnCount="0"
                                rows="{firstTable>/}"
                                selectionMode="MultiToggle"
                                ariaLabelledBy="title"
                                width="auto"
                            >
                                <t:extension>
                                    <OverflowToolbar>
                                        <ToolbarSpacer />
                                        <Button
                                            icon="sap-icon://add"
                                            press="onAdd1"
                                        />
                                        <Button
                                            icon="sap-icon://delete"
                                            press="onDelete1"
                                        />
                                    </OverflowToolbar>
                                </t:extension>

                                <t:rowMode>
                                    <rowmodes:Fixed />
                                </t:rowMode>
                                <t:columns>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Purchase code " />
                                        <t:template>
                                            <Input value="{firstTable>pCode}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label text="Contract No " />
                                        <t:template>
                                            <Input
                                                value="{firstTable>contractNo}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label text="Purchase method " />
                                        <t:template>
                                            <Input
                                                value="{firstTable>purchaseMethood}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Contract Subject (Procurement item code) "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>contractSubject}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Contract award date (dd.mm.yy.)  "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>contractAwardDate}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Contract expiry date (dd.mm.yy.) "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>contractExpireDate}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Total contract value, without VAT, thousand tenge "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>totalConValue}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="15rem">
                                        <Label
                                            text="Organizational and legal entity form of the supplier of GWS "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>orgAndLegal}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label text="Country of Residency " />
                                        <t:template>
                                            <Input value="{firstTable>COR}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Goods, works, services supplier name  "
                                        />
                                        <t:template>
                                            <Input value="{firstTable>GWS}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label text="BIN (IIN) of supplier " />
                                        <t:template>
                                            <Input value="{firstTable>BIN}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Address of physical / actual location of supplier "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>addOfPhy}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label text="Code of GWS as per SCP " />
                                        <t:template>
                                            <Input
                                                value="{firstTable>codeOfGWS}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Name and brief (additional) description of purchased goods, works and services "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>nameAndBrief}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Code for units of measure "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>codeForUnits}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Procurement scope in terms of volume "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>procScope}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Actual procurement amount in terms of monetary value excluding VAT, thousand tenge "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>actualProc}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Registration number of the State Registration Act of the Contract for carrying out of Subsoil Operations"
                                        />
                                        <t:template>
                                            <Input value="{firstTable>regNo}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Local goods manufacturer’s BIN/IIN, to whom the certificate СТ-КZ is issued "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>localGoods}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label text="CT-KZ certificate No. " />
                                        <t:template>
                                            <Input value="{firstTable>CTKZ}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Date of CT-KZ certificate issue, dd.mm.yyyy "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>dateOfCTKZ}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Local content in goods indicated in CT-KZ certificate, % "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>localConInGoods}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column width="11rem">
                                        <Label
                                            text="Local content in works (services), % "
                                        />
                                        <t:template>
                                            <Input
                                                value="{firstTable>localConInWS}"
                                            />
                                        </t:template>
                                    </t:Column>
                                </t:columns>
                            </t:Table>

                            <Toolbar id="_IDGenToolbar3">
                                <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                                <Button
                                    id="_IDGenButton01"
                                    text="Save"
                                    type="Emphasized"
                                />
                                <Button
                                    id="_IDGenButton02"
                                    text="Clear"
                                    type="Emphasized"
                                />
                                
                                 <Button
                                    
                                    text="Download csv"
                                    type="Emphasized"
                                />
                                 <Button
                                  
                                    text="Print Report"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                        <WizardStep
                            id="selectinfo"
                            validated="true"
                            title="Reporting period"
                            activate="additionalInfoValidation"
                        >
                            <t:Table
                                id="itemsTable2"
                                fixedColumnCount="0"
                                rows="{secondTable>/}"
                                selectionMode="MultiToggle"
                                ariaLabelledBy="title"
                            >
                                <t:extension>
                                    <OverflowToolbar>
                                        <ToolbarSpacer />
                                        <Button
                                            icon="sap-icon://add"
                                            press="onAdd2"
                                        />
                                        <Button
                                            icon="sap-icon://delete"
                                            press="onDelete2"
                                        />
                                    </OverflowToolbar>
                                </t:extension>

                                <t:columns>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Company Name" />
                                        <t:template>
                                            <Input
                                                value="{secondTable>companyName}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Name of goods, works and services"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>nameofGWS}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Unit" />
                                        <t:template>
                                            <Input value="{secondTable>unit}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Volume of purchase in physical terms"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>volofPur}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Actual volume of purchase excluding VAT in value terms, thousand tenge"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>actualvolofpurexcVAT}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Local content, in thousand tenge"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>localcontent}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Local content in work (service), %"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>localcontentinWS}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Name of the manufacturer of the goods to which the ST-KZ certificate was issued"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>nameofManufactur}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="IIN/BIN of the manufacturer of the goods to whom the ST-KZ certificate has been issued"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>IIN}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Certificate No. 'ST-KZ'" />
                                        <t:template>
                                            <Input
                                                value="{secondTable>certificateNo}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Date of issue of the ST-KZ certificate, (day, month, year)"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>DOI}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="GWS code according to SKP"
                                        />
                                        <t:template>
                                            <Input value="{secondTable>GWSCode}" />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="The region of the manufacturer of the goods CT-KZ"
                                        />
                                        <t:template>
                                            <Input
                                                value="{secondTable>regionofManufac}"
                                            />
                                        </t:template>
                                    </t:Column>
                                </t:columns>
                            </t:Table>

                            <Toolbar id="_IDGenToolbar2">
                                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                                <Button
                                    id="_IDGenButton1"
                                    text="Save"
                                    type="Emphasized"
                                />
                                <Button
                                    id="_IDGenButton2"
                                    text="Clear"
                                    type="Emphasized"
                                />
                                
                                 <Button
                                    
                                    text="Download csv"
                                    type="Emphasized"
                                />
                                 <Button
                                  
                                    text="Print Report"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                        <WizardStep
                            id="numberofemployee"
                            title="Number of employees working / involved for KPO activities in WKO"
                            validated="true"
                        >
                            <t:Table
                                id="itemsTable03"
                                fixedColumnCount="0"
                                rows="{thirdtable>/}"
                                selectionMode="MultiToggle"
                                ariaLabelledBy="title"
                            >
                                <t:extension>
                                    <OverflowToolbar>
                                        <ToolbarSpacer />
                                        <Button
                                            icon="sap-icon://add"
                                            press="onAdd3"
                                        />
                                        <Button
                                            icon="sap-icon://delete"
                                            press="onDelete3"
                                        />
                                    </OverflowToolbar>
                                </t:extension>
                                <t:columns>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Contractor Name" />
                                        <t:template>
                                            <Input
                                                value="{thirdtable>ContractorName}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Total Number of employees of the whole legal entity"
                                        />
                                        <t:template>
                                            <Input
                                                value="{thirdtable>Tnoe}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Involved for KPO activities in WKO of the whole legal entity"
                                        />
                                        <t:template>
                                            <Input
                                                value="{thirdtable>noe}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Involved for KPO activities in WKO of the whole legal entity"
                                        />
                                        <t:template>
                                            <Input
                                                value="{thirdtable>Tnofe}"
                                            />
                                        </t:template>
                                    </t:Column>
                                </t:columns>
                            </t:Table>

                            <Toolbar id="_IDGenToolbar005">
                                <ToolbarSpacer id="_IDGenToolbarSpacer06" />
                                <Button
                                    id="_IDGenButton05"
                                    text="Save"
                                    type="Emphasized"
                                />
                                <Button
                                    id="_IDGenButton005"
                                    text="Clear"
                                    type="Emphasized"
                                />
                                 <Button
                                    id="_IDGenButto"
                                    text="Download csv"
                                    type="Emphasized"
                                />
                                 <Button
                                  id="_IDGenButt05"
                                    text="Print Report"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                        <WizardStep
                            id="number"
                            title="NUMBER OF CONTRAСTOR'S EMPLOYEES 
"
                            validated="true"
                        >
                            <t:Table
                                id="itemsTable3"
                                fixedColumnCount="0"
                                rows="{fourthtable>/}"
                                selectionMode="MultiToggle"
                                ariaLabelledBy="title"
                            >
                                <t:extension>
                                    <OverflowToolbar>
                                        <ToolbarSpacer />
                                        <Button
                                            icon="sap-icon://add"
                                            press="onAdd4"
                                        />
                                        <Button
                                            icon="sap-icon://delete"
                                            press="onDelete4"
                                        />
                                    </OverflowToolbar>
                                </t:extension>
                                <t:columns>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Total number of employees (the whole legal entity)"
                                        />
                                        <t:template>
                                            <Input
                                                value="{fourthtable>totalnoofemploye}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Number of employees - RoK citizens (the whole legal entity)"
                                        />
                                        <t:template>
                                            <Input
                                                value="{fourthtable>noofemployerokcitizen}"
                                            />
                                        </t:template>
                                    </t:Column>
                                </t:columns>
                            </t:Table>

                            <Toolbar id="_IDGenToolbar4">
                                <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                                <Button
                                    id="_IDGenButton4"
                                    text="Save"
                                    type="Emphasized"
                                />
                                <Button
                                    id="_IDGenButton6"
                                    text="Clear"
                                    type="Emphasized"
                                />
                                
                                 <Button
                                    
                                    text="Download csv"
                                    type="Emphasized"
                                />
                                 <Button
                                  
                                    text="Print Report"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                        <WizardStep
                            id="form"
                            title="Form on RoK citizen employees’ payroll"
                            validated="true"
                        >
                            <t:Table
                                id="itemsTable4"
                                fixedColumnCount="0"
                                rows="{fifthTable>/}"
                                selectionMode="MultiToggle"
                                ariaLabelledBy="title"
                            >
                                <t:extension>
                                    <OverflowToolbar>
                                        <ToolbarSpacer />
                                        <Button
                                            icon="sap-icon://add"
                                            press="onAdd5"
                                        />
                                        <Button
                                            icon="sap-icon://delete"
                                            press="onDelete5"
                                        />
                                    </OverflowToolbar>
                                </t:extension>
                                <t:columns>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Company name" />
                                        <t:template>
                                            <Input
                                                value="{fifthTable>companyName}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label text="Reporting period" />
                                        <t:template>
                                            <Input
                                                value="{fifthTable>reportingperiod}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="Total payroll (%) of company’s employees (the whole legal entity)"
                                        />
                                        <t:template>
                                            <Input
                                                value="{fifthTable>totalpayrol}"
                                            />
                                        </t:template>
                                    </t:Column>
                                    <t:Column
                                        width="11rem"
                                        class="kkk"
                                    >
                                        <Label
                                            text="The share (in%) of Kazakhstan citizen employees’ payroll out of total payroll of company’s employees (the whole legal entity)"
                                        />
                                        <t:template>
                                            <Input
                                                value="{fifthTable>sharein}"
                                            />
                                        </t:template>
                                    </t:Column>
                                </t:columns>
                            </t:Table>
                             <Toolbar id="_IDGenToolbar7">
                                <ToolbarSpacer id="_IDGenToolbarSpacer8" />
                                <Button
                                    id="_IDGenButt"
                                    text="Save"
                                    type="Emphasized"
                                />
                                <Button
                                   
                                    text="Clear"
                                    type="Emphasized"
                                />
                                
                                 <Button
                                    
                                    text="Download csv"
                                    type="Emphasized"
                                />
                                 <Button
                                  
                                    text="Print Report"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                    </Wizard>
                </content>
                <footer>
                    <Toolbar id="_IDGenToolbar5">
                        <ToolbarSpacer id="_IDGenToolbarSpacer6" />
                        <Button
                            id="_IDGenButton5"
                            text="Submit"
                            type="Emphasized"
                        />
                    </Toolbar>
                </footer>
            </Page>
        </pages>
    </NavContainer>
</mvc:View>