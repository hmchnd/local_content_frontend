                                        text="{SubmitcommentsModel>Comment}" 
                                    />
                                </time:content>
                            </time:Timeline>
                        </VBox>
                    </f:SidePanelItem>
                    <f:SidePanelItem
                        icon=""
                        text=""
                        enabled="false"
                    />
                </f:items>
            </f:SidePanel>
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    enabled="true"
                    press="onSubmit"
                    text="{i18n>submitforApproval}"
                    type="Emphasized"
                />
            </Toolbar>
        </footer>
    </Page>
</mvc:View>
